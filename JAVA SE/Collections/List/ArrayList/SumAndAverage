import java.util.Arrays;
import java.util.List;

public class TestThreads {
    public static void main(String[] args) {

        List<Integer> lst = Arrays.asList(4, 5, 0, 9, 8, 10);

        int count = 0;
        for(int i : lst)
            count += i;

        System.out.println("Sum of list elements: " + count);
        System.out.println("Average of list element: " + count / lst.size());

    }
}

//Output :  sum = 36
//          average = 6

